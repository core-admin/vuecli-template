{
  "name": "vuecli5-template-ts",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "reinstall": "pnpm clear && pnpm bootstrap",
    "start": "pnpm start:dev",
    "dev": "pnpm start:dev",
    "serve": "pnpm start:dev",
    "start:dev": "cross-env NODE_ENV=development vue-cli-service serve",
    "start:test": "cross-env NODE_ENV=development vue-cli-service serve --mode=test",
    "start:preview": "cross-env NODE_ENV=development vue-cli-service serve --mode=preview",
    "start:production": "cross-env NODE_ENV=development vue-cli-service serve --mode=production",
    "build:dev": "cross-env NODE_ENV=production vue-cli-service build --mode=development",
    "build:test": "cross-env NODE_ENV=production vue-cli-service build --mode=test",
    "build:preview": "cross-env NODE_ENV=production vue-cli-service build --mode=preview",
    "build:production": "cross-env NODE_ENV=production vue-cli-service build --mode=production",
    "build:test:report": "cross-env NODE_ENV=production vue-cli-service build --mode=test --report",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "format": "pnpm run lint:vue && pnpm run lint:eslint && pnpm run lint:prettier",
    "lint:vue": "vue-cli-service lint",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{vue,ts,tsx,js,jsx,less,scss,css,html,json}\"",
    "clear": "rimraf node_modules dist package-lock.json yarn.lock pnpm-lock.json pnpm-lock.yaml",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "previewWebpackConfig:dev": "vue-cli-service inspect --mode development > devConfig.js",
    "previewWebpackConfig:prod": "vue-cli-service inspect --mode production > prodConfig.js"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.2.2",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.5.1",
    "core-js": "^3.25.5",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.5",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.5",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "tj-design-vue": "^1.1.29",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.8.6",
    "@types/qs": "^6.9.7",
    "@types/webpack-env": "^1.18.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-typescript": "^9.1.0",
    "babel-plugin-import": "^1.13.8",
    "compression-webpack-plugin": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "less": "^4.1.3",
    "less-loader": "^8.1.1",
    "lint-staged": "^12.5.0",
    "postcss": "^8.4.18",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rimraf": "^5.0.5",
    "style-resources-loader": "^1.5.0",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.27",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8.1.0"
  }
}